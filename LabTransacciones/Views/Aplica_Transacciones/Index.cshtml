@model IEnumerable<LabTransacciones.Models.Aplica_Transacciones>

@{
    ViewBag.Title = "Index";
    var numeroError = TempData["Error"];
    if (numeroError != null && (int)numeroError != 0)
    {
        <script>javascript: alert(`error @numeroError`);</script>
    }
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cedula)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SiglaExam)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cedula)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SiglaExam)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { Cedula = item.Cedula, SiglaExam = item.SiglaExam }) |
                @Html.ActionLink("Details", "Details", new { Cedula = item.Cedula, SiglaExam = item.SiglaExam }) |
                @Html.ActionLink("Delete", "Delete", new { Cedula = item.Cedula, SiglaExam = item.SiglaExam })
            </td>
        </tr>
    }

</table>
